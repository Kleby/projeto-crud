<!DOCTYPE html>
<html lang="pt-br">
<%- include('partials/head.ejs')  %> 
<body>
  <%- include('partials/header.ejs') %>
  <h1> <%= title %> </h1>

  <ul>
    <% users.forEach( user => { %>
      <li>
        <%= user.name %> - <%= user.email %> - Idade:<%= user.age %> 
        <a href="/edit?id=<%= user._id %>">[Editar]</a> 
      </li>
    <% }); %>
  </ul>
  <%- include('partials/foot.ejs') %> 
</body>
</html>
